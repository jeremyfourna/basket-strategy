		"bootstrap": "^4.0.0-beta",
		"d3-selection": "^1.1.0",
		"d3-shape": "^1.2.0",
		"d3-transition": "^1.1.0",
		"firebase": "^4.3.0",
		"http-server": "^0.10.0",
		"jquery": "^3.2.1",
		"ramda": "^0.24.1"
	},
